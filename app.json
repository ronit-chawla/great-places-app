{
  "expo": {
    "name": "GreAT PlaCES",
    "description":"An app to share places you visit",
    "slug": "great-places",
    "privacy":"public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 1000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.notanapp.great-places",
      "buildNumber":"1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package":"com.notanapp.great_places",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/places-adaptive.png",
        "backgroundColor": "#171717"
      },
      "permissions":[
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
